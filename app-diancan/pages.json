{
    "easycom": {
        "^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
    },
    "pages": [
        //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
        {
            "path": "pages/home/index",
            "style": {
                "navigationBarTitleText": "首页",
                "transparentTitle": "auto",
                "navigationStyle": "custom"
            }
        },
        {
            "path": "pages/place-order/index",
            "style": {
                "navigationBarTitleText": "点餐"
            }
        },
        {
            "path": "pages/place-order/pay-the-bill",
            "style": {
                "navigationBarTitleText": "确认订单",
                "transparentTitle": "auto",
                "navigationStyle": "custom"
            }
        },
        {
            "path": "pages/place-order/search",
            "style": {
                "navigationBarTitleText": "搜索"
            }
        },
        {
            "path": "pages/order-form/index",
            "style": {
                "navigationBarTitleText": "订单",
                "mp-alipay": {
                    "allowsBounceVertical": "NO"
                }
            }
        },
        {
            "path": "pages/order-form/order-bill",
            "style": {
                "navigationBarTitleText": "开发票"
            }
        }
    ],
    "globalStyle": {
        "navigationBarTitleText": "扫码快点餐",
        "navigationBarBackgroundColor": "#FF7250",
        "navigationBarTextStyle": "white",
        "backgroundColor": "#F8F8F8"
    }
}
